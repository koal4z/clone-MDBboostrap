{
  "extends": [
    "react-app",
    "prettier",
    "prettier/react",
    "airbnb",
    "eslint:recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "plugins": ["jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "linebreak-style": ["off", "windows"],
    "comma-dangle": ["error", "never"],
    "object-curly-newline": ["error", { "multiline": true }]
  }
}
